{
  "private": true,
  "browserslist": [
    "extends @wordpress/browserslist-config"
  ],
  "engines": {
    "node": ">=12.0.0"
  },
  "scripts": {
    "build": "cross-env NODE_ENV=development run-s mix",
    "build:production": "cross-env NODE_ENV=production run-s clean mix",
    "start": "cross-env NODE_ENV=development run-s \"mix -- --watch\"",
    "hot": "cross-env NODE_ENV=development run-s build mix:hot",
    "mix": "webpack --progress --hide-modules --config=node_modules/laravel-mix/setup/webpack.config.js",
    "mix:hot": "webpack-dev-server --inline --hot --config=node_modules/laravel-mix/setup/webpack.config.js",
    "clean": "run-p clean:*",
    "clean:dist": "rimraf dist",
    "clean:cache": "rimraf storage/framework/cache/*.php storage/framework/cache/data/*.php",
    "clean:views": "rimraf storage/framework/views/*.php",
    "translate": "run-s -c translate:*",
    "translate:pot": "wp i18n make-pot . ./resources/languages/sage.pot --ignore-domain --include=\"app,resources/assets,resources/views\"",
    "translate:js": "wp i18n make-json ./resources/languages --no-purge --pretty-print"
  },
  "devDependencies": {
    "@wordpress/babel-preset-default": "^4.17.0",
    "@wordpress/browserslist-config": "^2.7.0",
    "@wordpress/dependency-extraction-webpack-plugin": "^2.8.0",
    "babel-eslint": "^10.1.0",
    "browser-sync": "^2.26.13",
    "browser-sync-webpack-plugin": "^2.0.1",
    "cross-env": "^7.0.2",
    "css-reset-and-normalize": "^2.1.0",
    "eslint": "^7.7.0",
    "eslint-plugin-import": "^2.22.0",
    "laravel-mix": "^5.0.4",
    "laravel-mix-copy-watched": "^2.2.4",
    "laravel-mix-postcss-config": "^1.0.3",
    "laravel-mix-purgecss": "^5.0.0",
    "npm-run-all": "^4.1",
    "object-fit-images": "^3.2.4",
    "picturefill": "^3.0.3",
    "purgecss-with-wordpress": "^2.1.0",
    "rimraf": "^3.0.2",
    "vue-template-compiler": "^2.6.11"
  },
  "dependencies": {
    "jquery": "^3.5.1",
    "jquery-match-height": "^0.7.2",
    "postcss-alias": "^2.0.0",
    "postcss-apply-class": "^0.1.6",
    "postcss-aspect-ratio": "^1.0.2",
    "postcss-assets": "^5.0.0",
    "postcss-atroot": "^0.1.3",
    "postcss-calc": "^7.0.1",
    "postcss-color-function": "^4.1.0",
    "postcss-color-scale": "^1.1.2",
    "postcss-combine-media-query": "^1.0.0",
    "postcss-conditionals": "^2.1.0",
    "postcss-current-selector": "0.0.3",
    "postcss-custom-media": "^7.0.8",
    "postcss-define-property": "^0.5.0",
    "postcss-discard-comments": "^4.0.2",
    "postcss-discard-duplicates": "^4.0.2",
    "postcss-discard-empty": "^4.0.1",
    "postcss-each": "^0.10.0",
    "postcss-extend": "^1.0.5",
    "postcss-fixes": "^2.0.1",
    "postcss-focus": "^4.0.0",
    "postcss-font-magician": "^2.3.1",
    "postcss-for": "^2.1.1",
    "postcss-google-color": "^2.0.1",
    "postcss-if-media": "^1.0.3",
    "postcss-import": "^12.0.1",
    "postcss-import-ext-glob": "^1.1.0",
    "postcss-important-shorthand": "^2.0.0",
    "postcss-inline-media": "^1.3.0",
    "postcss-media-fn": "^3.0.0",
    "postcss-media-variables": "^2.0.1",
    "postcss-mixins": "^6.2.3",
    "postcss-morphicon": "^1.0.0",
    "postcss-nested": "^4.2.1",
    "postcss-nested-ancestors": "^2.0.0",
    "postcss-nested-vars": "^1.0.0",
    "postcss-normalize": "^8.0.1",
    "postcss-preset-env": "^6.7.0",
    "postcss-responsive-type": "^1.0.0",
    "postcss-short": "^5.0.0",
    "postcss-simple-vars": "^5.0.2",
    "postcss-sort-media-queries": "^1.0.15",
    "postcss-type-scale": "^1.0.0",
    "postcss-utilities": "^0.8.4",
    "postcss-zindex": "^4.0.1",
    "remodal": "^1.1.1",
    "request": "^2.88.0",
    "scrollreveal": "^4.0.5",
    "slick-carousel": "^1.8.1",
    "string-strip-html": "^4.3.16",
    "svg4everybody": "^2.1.9"
  }
}
